<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="comps413f.android.flyingshark"
    android:versionCode="1"
    android:versionName="1.0">

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/android_00s"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity android:name="comps413f.android.flyingshark.FlyingSharkActivity"
            android:label="FlyingShark">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="comps413f.android.flyingshark.MainActivity"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name="comps413f.android.flyingshark.MenuActivity"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name="comps413f.android.flyingshark.ResumeActivity"
            android:label="Resume"></activity>
        <activity
            android:name="comps413f.android.flyingshark.FlyingSharkPrefActivity"
            android:label="@string/app_name">
        </activity>
    </application>

</manifest>